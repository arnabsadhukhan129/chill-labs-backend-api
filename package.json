{
  "name": "chill-labs-api-staging",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start:prod": "NODE_ENV=production node dist/src/loadEnv.js",
    "start:dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/loadEnv.ts",
    "start:uat": "NODE_ENV=uat node dist/src/loadEnv.js",
    "staging": "pm2 start npm --name chill-labs-api-staging -- run start:prod",
    "staging:stop": "pm2 stop chill-labs-api-staging && pm2 delete chill-labs-api-staging",
    "build": "tsc && cpx 'templates/**/*.html' dist/templates && cpx 'models/**/*' dist/models && cpx 'types/**/*' dist/types && cpx '.env.production' dist/",
    "build:uat": "tsc && cpx 'templates/**/*.html' dist/templates && cpx 'models/**/*' dist/models && cpx 'types/**/*' dist/types && cpx '.env.uat' dist/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "create-super-admin": "ts-node -r tsconfig-paths/register src/scripts/createSuperAdmin.ts"
  },
  "keywords": [],
  "author": "Atanu Paul",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/i18n": "^0.13.12",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^7.0.4",
    "@types/redis": "^4.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "chokidar": "^4.0.1",
    "cpx": "^1.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@aws-sdk/client-s3": "^3.685.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "force": "^0.0.3",
    "google-auth-library": "^9.15.1",
    "i18n": "^0.15.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "nodemailer": "^7.0.11",
    "redis": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3"
  },
  "engines": {
    "node": ">=22.15.0",
    "npm": "~10.2.4"
  }
}
